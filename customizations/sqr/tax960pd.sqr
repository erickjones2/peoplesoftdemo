Begin-Procedure Get-ExemptSal-Amt
  let #k=0
  while #k <= length($Exempt_Sal_Code)
     if $Comb_Code = ExemptSalAmts.Exempt_Sal_Cd(#k)
        let $Com_Var_2 = $Com_Var_2 || ExemptSalAmts.Exempt_Sal_Cd(#k)
        let #Com_Var_1 = #Com_Var_1 + ExemptSalAmts.Exempt_Sal_Amt(#k)
        if $Comb_Code = 'E'
           let ExemptSalAmts.Exempt_Sal_Cd(#k) = ' '
           let ExemptSalAmts.Exempt_Sal_Amt(#k) = 0
           let #p=0
           while #p <= length($Exempt_Sal_Code)
              if ExemptSalAmts.Exempt_Sal_Cd(#p) = 'F'
                 let ExemptSalAmts.Exempt_Sal_Cd(#p) = ' '
                 let ExemptSalAmts.Exempt_Sal_Amt(#p) = 0
                 break
              end-if
              let #p=#p + 1
           end-while
        else
           if $Comb_Code = 'F'
              let ExemptSalAmts.Exempt_Sal_Cd(#k) = ' '
              let ExemptSalAmts.Exempt_Sal_Amt(#k) = 0
           end-if
        end-if
        let ExemptSalAmts.Exempt_Sal_Pr(#k) = 'Y'
        break
     end-if
     let #k = #k + 1
  end-while
end-procedure


Begin-Procedure Get-ExemptSal-Code

  let #i=0
  while #i < #index_exempt_sal
     let #k=0
     while #k < #index_exempt_sal
        if ExemptSalCode.Exempt_Sal_Codes(#i) < ExemptSalCode.Exempt_Sal_Codes(#k)
           let $TempSal = ExemptSalCode.Exempt_Sal_Codes(#i)
           let ExemptSalCode.Exempt_Sal_Codes(#i) = ExemptSalCode.Exempt_Sal_Codes(#k)
           let ExemptSalCode.Exempt_Sal_Codes(#k) = $TempSal
        end-if
        let #k=#k+1
     end-while
     let #i=#i+1
  end-while
     
  let #i=0
  while #i < #index_exempt_sal
     if INSTR(ExemptSalCode.Exempt_Sal_Codes(#i),$Exempt_Sal_Code,1) = 0
        let $Exempt_Sal_Code = $Exempt_Sal_Code || ExemptSalCode.Exempt_Sal_Codes(#i)
     end-if
     let #i = #i + 1
  end-while

  clear-array name=ExemptSalAmts
  let #i = 1
  while #i <= length($Exempt_Sal_Code)

     let ExemptSalAmts.Exempt_Sal_Cd(#i - 1) = substr($Exempt_Sal_Code,#i,1)
     let #p = 0
     while #p < #tmp_idx
        if ExemptSalAmts.Exempt_Sal_Cd(#i - 1) = TMP_SAL_CODES.TMP_CODE(#p)
           let $TmpSalCodes = TMP_SAL_CODES.TMP_BOX(#p)
           evaluate $TmpSalCodes
              when = '1601'
                 let ExemptSalAmts.Exempt_Sal_Amt(#i - 1) = #Exempt_Sal_01
                 break
              when = '1602'
                 let ExemptSalAmts.Exempt_Sal_Amt(#i - 1) = #Exempt_Sal_02
                 break
              when = '1603'
                 let ExemptSalAmts.Exempt_Sal_Amt(#i - 1) = #Exempt_Sal_03
                 break
              when = '1604'
                 let ExemptSalAmts.Exempt_Sal_Amt(#i - 1) = #Exempt_Sal_04
                 break
              when = '1605'
                 let ExemptSalAmts.Exempt_Sal_Amt(#i - 1) = #Exempt_Sal_05
                 break
              when = '1606'
                 let ExemptSalAmts.Exempt_Sal_Amt(#i - 1) = #Exempt_Sal_06
                 break
              when = '1607'
                 let ExemptSalAmts.Exempt_Sal_Amt(#i - 1) = #Exempt_Sal_07
                 break
              when = '1608'
                 let ExemptSalAmts.Exempt_Sal_Amt(#i - 1) = #Exempt_Sal_08
                 break
              when = '1609'
                 let ExemptSalAmts.Exempt_Sal_Amt(#i - 1) = #Exempt_Sal_09
                break
              when = '1610'
                 let ExemptSalAmts.Exempt_Sal_Amt(#i - 1) = #Exempt_Sal_10
                break
           end-evaluate
        end-if
        let #p = #p +1
     end-while
     let #i=#i+1
  end-while
  
End-Procedure


begin-procedure  Common-Printing-2016
  if #Exempt_Counter = 0
     move #Com_var_1 to $Exempt_Sal                      09999999999
     move $Com_var_2 to $Exempt_Sal_Cd
     let #Exempt_Counter = #Exempt_Counter + 1
     let #Exempt_Sal-T = #Exempt_Sal-T + #Com_Var_1
  else
      if #Exempt_Counter = 1
         move #Com_var_1 to $Exempt_Sal_A                09999999999
         move $Com_var_2 to $Exempt_Sal_Cd_A
         let #Exempt_Counter = #Exempt_Counter + 1
         let #Exempt_Sal-T = #Exempt_Sal-T + #Com_Var_1
      else
         if #Exempt_Counter = 2
            move #Com_var_1 to $Exempt_Sal_B             09999999999
            move $Com_var_2 to $Exempt_Sal_Cd_B
            let #Exempt_Counter = #Exempt_Counter + 1         
            let #Exempt_Sal-T = #Exempt_Sal-T + #Com_Var_1
         end-if
      end-if
  end-if
end-procedure


