  move 0 to $Exempt_Sal_A                                       09999999999
  move 0 to $Exempt_Sal_B                                       09999999999
  clear-array name=ExemptSalAmts
  do Get-ExemptSal-Code
  if instr($Exempt_Sal_Code,'E',1) <> 0
     let $Comb_Code ='E'
     let $Com_Var_2 =''
     let #Com_Var_1 = 0
     do Get-ExemptSal-Amt
     do Common-Printing-2016
     if instr($Exempt_Sal_Code,'F',1) <> 0 and length($Exempt_Sal_Code) = 4
        move 0 to #i
           while #i < length($Exempt_Sal_Code) and #Exempt_Counter < 3
           if ExemptSalAmts.Exempt_Sal_Cd(#i) <> ' '
              let #Com_Var_1 = ExemptSalAmts.Exempt_Sal_Amt(#i)
              let $Com_Var_2 = ExemptSalAmts.Exempt_Sal_Cd(#i)
              do Common-Printing-2016
           end-if
           let #i = #i + 1
        end-while
     end-if   
  else
     if instr($Exempt_Sal_Code,'F',1) <> 0
        let $Comb_Code ='F'
        let $Com_Var_2 =''
        let #Com_Var_1 = 0
        do Get-ExemptSal-Amt
        do Common-Printing-2016
     end-if
  end-if
  if length($Exempt_Sal_Code) < 4
     move 0 to #i
     while #i < length($Exempt_Sal_Code) and #Exempt_Counter < 3
        if ExemptSalAmts.Exempt_Sal_Cd(#i) <> ' '
           let #Com_Var_1 = ExemptSalAmts.Exempt_Sal_Amt(#i)
           let $Com_Var_2 = ExemptSalAmts.Exempt_Sal_Cd(#i)
           do Common-Printing-2016
        end-if
        let #i = #i + 1
     end-while
  else
     if length($Exempt_Sal_Code) = 4
        let #PrintExemptSal = 0
        if instr($Exempt_Sal_Code,'A',1) <> 0
           let $Comb_Code ='A'
           let $Com_Var_2 =''
           let #Com_Var_1 = 0
           do Get-ExemptSal-Amt
           if instr($Exempt_Sal_Code,'B',1) <> 0 
              let $Comb_Code ='B'
              do Get-ExemptSal-Amt
              let #PrintExemptSal = 1
           else
              if instr($Exempt_Sal_Code,'G',1) <> 0 
                 let $Comb_Code ='G'
                 do Get-ExemptSal-Amt
                 let #PrintExemptSal = 1
              else
                 if instr($Exempt_Sal_Code,'H',1) <> 0
                    let $Comb_Code ='H'
                    do Get-ExemptSal-Amt
                   let #PrintExemptSal = 1
                 end-if
              end-if
           end-if
        else
           if instr($Exempt_Sal_Code,'B',1) <> 0
              let $Comb_Code ='B'
              let $Com_Var_2 =''
              let #Com_Var_1 = 0
              do Get-ExemptSal-Amt
              if instr($Exempt_Sal_Code,'G',1) <> 0
                 let $Comb_Code ='G'
                 do Get-ExemptSal-Amt
                 let #PrintExemptSal = 1
              else
                 if instr($Exempt_Sal_Code,'H',1) <> 0
                    let $Comb_Code ='H'
                    do Get-ExemptSal-Amt
                    let #PrintExemptSal = 1
                  end-if
              end-if
           else
              if instr($Exempt_Sal_Code,'G',1) <> 0
                 let $Comb_Code ='G'
                 let $Com_Var_2 =''
                 let #Com_Var_1 = 0
                 do Get-ExemptSal-Amt
                 if instr($Exempt_Sal_Code,'H',1) <> 0
                    let $Comb_Code ='H'
                    do Get-ExemptSal-Amt
                    let #PrintExemptSal = 1
                  end-if
              end-if
           end-if
        end-if
        if #PrintExemptSal = 1
           let $Comb_Code = $Com_Var_2
           let #Comb_Amt = #Com_Var_1
           move 0 to #i
           while #i < length($Exempt_Sal_Code) and #Exempt_Counter < 2
              if ExemptSalAmts.Exempt_Sal_Cd(#i) <> ' '
                and
                 ExemptSalAmts.Exempt_Sal_Pr(#i) <> 'Y'
                 let #Com_Var_1 = ExemptSalAmts.Exempt_Sal_Amt(#i)
                 let $Com_Var_2 = ExemptSalAmts.Exempt_Sal_Cd(#i)
                 do Common-Printing-2016
              end-if
              let #i = #i + 1
           end-while
           let $Com_Var_2 = $Comb_Code
           let #Com_Var_1 = #Comb_Amt
           do Common-Printing-2016
        else   
           move 0 to #i
           while #i < length($Exempt_Sal_Code) and #Exempt_Counter < 2
              if ExemptSalAmts.Exempt_Sal_Cd(#i) <> ' '
                 let #Com_Var_1 = ExemptSalAmts.Exempt_Sal_Amt(#i)
                 let $Com_Var_2 = ExemptSalAmts.Exempt_Sal_Cd(#i)
                 do Common-Printing-2016
              end-if
              let #i = #i + 1
           end-while
        end-if
     else 
        move 0 to #i
        while #i < length($Exempt_Sal_Code) and #Exempt_Counter < 2
           if ExemptSalAmts.Exempt_Sal_Cd(#i) <> ' '
              let #Com_Var_1 = ExemptSalAmts.Exempt_Sal_Amt(#i)
              let $Com_Var_2 = ExemptSalAmts.Exempt_Sal_Cd(#i)
              do Common-Printing-2016
           end-if
           let #i = #i + 1
        end-while
     end-if
  end-if 
 
  if Rtrim($Exempt_Sal_Cd, ' ') = ''
    move ' '                 to $Exempt_Sal_Cd
  end-if
  
  if Rtrim($Exempt_Sal_Cd_A, ' ') = ''
