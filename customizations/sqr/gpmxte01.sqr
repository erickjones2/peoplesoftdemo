!                let #rest_amt_te = #rest_amt_te + &D.GPMX_REST_AMT
!                let #in_hrs_te = #in_hrs_te + &D.GPMX_DBL_IN_HRS
!                let #in_amt_te = #in_amt_te + &D.GPMX_DBL_IN_AMT
!                                  
!FROM PS_GPMX_OVR_DAILY D, PS_GPMX_OVRTME_CAL F, PS_NAMES C1
