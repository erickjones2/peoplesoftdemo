 print #trip_hrs_te  (,{c5A})  edit 99999.99
 print #trip_amt_te  (,{c6})  edit 999999.99
 print #hol_hrs_te  (,{c7A})   edit 99999.99
 print #hol_amt_te  (,{c8})   edit 999999.99
 print #rest_hrs_te  (,{c9A})  edit 99999.99
