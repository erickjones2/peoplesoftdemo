!**************************************
begin-procedure Print-Error-Line
!**************************************

  if rtrim($Company, ' ') <> rtrim($priorCompany, ' ')
    if $ErrorStart = 'N'
      NEW-PAGE
    end-if
    do Get-Company-Data
  end-if

  next-listing need=3

  if rtrim($Company, ' ') <> rtrim($priorCompany, ' ')
    move $Company to $priorCompany
  end-if

  print $EmplID           (+1,1)

  if $EmplLoadErr   = 'Y'
    print $EmplName       (0,15)
    print $SIN            (0,65)   edit xxx-xxx-xxx
    move 'N' to $EmplLoadErr
  end-if

  if $IDLoadErr     = 'Y'
    print $TaxFormID      (0,81)
    print $WageLossPlan   (0,88)
    print $Province       (0,95)
    print $Reporting_ID   (0,106)
    move 'N' to $IDLoadErr
  end-if

  if $DetailLoadErr = 'Y'
    print $TaxFormID      (0,81)
    print $WageLossPlan   (0,88)
    print $Province       (0,95)
    print $Tbl_Box_Num    (0,124)
    print #BoxAmt         (0,129)  edit b9999999999.99
    print $BoxText        (0,142)
    move 'N' to $DetailLoadErr
  end-if

  print $ErrorMsg         (0,153)

  move 'N' to $ErrorStart

end-procedure


