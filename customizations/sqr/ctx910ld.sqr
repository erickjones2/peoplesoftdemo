!**************************************
begin-procedure Multiple-Box14-Exemptions
!**************************************

 let #Interim_14 = 0
 let #Interim_71 = 0
 let #Interim_BT = 0
 let #Interim_TOTBT = 0
 if $Fully-Exempt = 'Y'
   let $Tax_Class = 'TT4'
 else
   let $Tax_Class = 'CIT'
 end-if

begin-select
TAX.TAX_CLASS_CAN
TAX.TXGRS_CUR
DTA14X.PCT_EXEMPT
CHK.CHECK_DT
   if &TAX.TAX_CLASS_CAN = 'TT4'
     let #Interim_14 = #Interim_14 + Round(&TAX.TXGRS_CUR,2)
   end-if
   if &TAX.TAX_CLASS_CAN = 'TT4' and &DTA14X.PCT_Exempt <> 0
     let #Interim_71 = #Interim_71 + Round((&TAX.TXGRS_CUR * ((&DTA14X.PCT_Exempt) / 100)),2)
   end-if

FROM PS_PAY_CHECK    CHK,
     PS_PAY_TAX_CAN  TAX,
     PS_CAN_TAX_DATA DTA14X,
     PS_PAY_CALENDAR CAL
WHERE CHK.EMPLID      = $Emplid
AND   CAL.COMPANY     = CHK.COMPANY
AND   CAL.PAYGROUP    = CHK.PAYGROUP
AND   CAL.PAY_END_DT  = CHK.PAY_END_DT
AND   CAL.CHECK_DT   >= $BeginDate
