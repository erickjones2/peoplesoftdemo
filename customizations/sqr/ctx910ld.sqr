!**************************************
begin-procedure CPP-for-RL
!**************************************

 let #Interim_CPPRL  = 0
 let #Interim_Total_CPPQPP  = 0
 let #Row_CPPQPP = 0
 let #Interim_G = 0

begin-select
TAXCPPRL.TAX_CLASS_CAN
TAXCPPRL.NLGRS_CUR
DTACPPRL.PCT_EXEMPT
DTACPPRL.USE_FOR_CPP
CHKCPPRL.CHECK_DT
CALCPPRL.CHECK_DT

  if  &TAXCPPRL.TAX_CLASS_CAN = 'CPP'
      if  &DTACPPRL.USE_FOR_CPP = 'Y'
          let #Row_CPPQPP = Round((&TAXCPPRL.NLGRS_CUR * ((100 - &DTACPPRL.PCT_Exempt) / 100)),2)
      else
          let #Row_CPPQPP =  Round(&TAXCPPRL.NLGRS_CUR,2)
      end-if

      if  #Interim_Total_CPPQPP >= #QPP_Max_Earns
          add 0 to #Interim_CPPRL
      else
          if Round((#Interim_Total_CPPQPP + #Row_CPPQPP),2) >=  #QPP_Max_Earns
              let #Interim_CPPRL = #Interim_CPPRL + Round ((#QPP_Max_Earns - #Interim_Total_CPPQPP),2)
          else
              let #Interim_CPPRL = #Interim_CPPRL + Round (#Row_CPPQPP,2)
          end-if
      end-if
  else
      if  &DTACPPRL.USE_FOR_CPP = 'Y'
          let #Row_CPPQPP = Round((&TAXCPPRL.NLGRS_CUR * ((100 - &DTACPPRL.PCT_Exempt) / 100)),2)
      else
          let #Row_CPPQPP = Round(&TAXCPPRL.NLGRS_CUR,2)
      end-if

      if  #Interim_Total_CPPQPP >= #QPP_Max_Earns
          add 0 to #Interim_G
      else
          if Round((#Interim_Total_CPPQPP + #Row_CPPQPP),2) >=  #QPP_Max_Earns
              let #Interim_G = #Interim_G + Round ((#QPP_Max_Earns - #Interim_Total_CPPQPP),2)
          else
              let #Interim_G = #Interim_G + Round (#Row_CPPQPP,2)
          end-if
      end-if
  end-if

  add #Row_CPPQPP to #Interim_Total_CPPQPP
  if #Interim_Total_CPPQPP >= #QPP_Max_Earns
     move #QPP_Max_Earns to #Interim_Total_CPPQPP
  end-if

FROM PS_PAY_CHECK    CHKCPPRL,
     PS_PAY_TAX_CAN  TAXCPPRL,
     PS_CAN_TAX_DATA DTACPPRL,
     PS_PAY_CALENDAR CALCPPRL
WHERE CHKCPPRL.EMPLID      = $Emplid
AND   CALCPPRL.COMPANY     = CHKCPPRL.COMPANY
AND   CALCPPRL.PAYGROUP    = CHKCPPRL.PAYGROUP
AND   CALCPPRL.PAY_END_DT  = CHKCPPRL.PAY_END_DT
AND   CALCPPRL.CHECK_DT   >= $BeginDate
AND   CALCPPRL.CHECK_DT   <= $AsOfDate
AND   CHKCPPRL.PAYCHECK_STATUS IN ('F','R','A')
AND   TAXCPPRL.COMPANY     = CHKCPPRL.COMPANY
AND   TAXCPPRL.PAYGROUP    = CHKCPPRL.PAYGROUP
AND   TAXCPPRL.PAY_END_DT  = CHKCPPRL.PAY_END_DT
AND   TAXCPPRL.OFF_CYCLE   = CHKCPPRL.OFF_CYCLE
AND   TAXCPPRL.PAGE_NUM    = CHKCPPRL.PAGE_NUM
AND   TAXCPPRL.LINE_NUM    = CHKCPPRL.LINE_NUM
AND   TAXCPPRL.SEPCHK      = CHKCPPRL.SEPCHK
AND   (TAXCPPRL.TAX_CLASS_CAN  = 'CPP' OR
       TAXCPPRL.TAX_CLASS_CAN  = 'QPP')
AND   DTACPPRL.EMPLID          = $Emplid
AND   DTACPPRL.COMPANY         = &A.COMPANY
AND   DTACPPRL.EFFDT           = (SELECT MAX(DTACPPRLA.EFFDT)
                               FROM   PS_CAN_TAX_DATA DTACPPRLA
                               WHERE  DTACPPRLA.EMPLID  = $Emplid
                               AND    DTACPPRLA.COMPANY = &A.COMPANY
                               AND    DTACPPRLA.EFFDT  <= CALCPPRL.CHECK_DT)
ORDER BY CALCPPRL.CHECK_DT

end-select

end-procedure


!*************************************
begin-procedure Proc-Multi-T4-Box26
!*************************************

  do Get-CPP-NLGrs-Totals

  do Update-Array-CPP-Max-ByTotal

  if $CPP_MaxOut_Found = 'Y'
    do Update-Array-CPP-NLGrs-Adj
  else
    if #CPP_Contrib_NLGrs_Total >= #CPP_Max_Earns
      do Find-CPP-Max-Out-Cheque
      if $CPP_MaxOut_Found = 'Y'
        do Update-Array-CPP-NLGrs-Adj
      end-if
    else
      do CPP-NLGrs-Adj-with-Box14
    end-if
  end-if

end-procedure


!**************************************
begin-procedure Get-CPP-NLGrs-Totals
!**************************************

  let #cpp_load     = 0
  let #max_cpp_load = 0
  let #CPP_Contrib_NLGrs_Total = 0

begin-SELECT
TB.PROVINCE
TB.WAGE_LOSS_PLAN
SUM(TB.NLGRS_CUR)             &CPP_NLGRS_SUM
SUM(TB.TXGRS_CUR)             &CPP_TXGRS_SUM
SUM(TB.TAX_CUR)               &CPP_TAX_SUM

   put &TB.PROVINCE &TB.WAGE_LOSS_PLAN &CPP_NLGRS_SUM &CPP_TXGRS_SUM &CPP_TAX_SUM
       into EE_CPP_Totals(#cpp_load) Province WLP CPP_NLGrs_Totals CPP_TxGrs_Totals CPP_Tax_Totals

   add 1 to #cpp_load
   move #cpp_load to #max_cpp_load

   let #CPP_Contrib_NLGrs_Total = &CPP_NLGRS_SUM + #CPP_Contrib_NLGrs_Total


FROM PS_PAY_CHECK    TA,
     PS_PAY_TAX_CAN  TB,
     PS_PAY_CALENDAR TC

WHERE TA.COMPANY     = &A.COMPANY
  AND TA.EMPLID      = $Emplid
  AND TA.COMPANY     = TC.COMPANY
  AND TA.PAYGROUP    = TC.PAYGROUP
  AND TA.PAY_END_DT  = TC.PAY_END_DT
  AND TC.CHECK_DT   >= $BeginDate
  AND TC.CHECK_DT   <= $AsOfDate
  AND TA.COMPANY     = TB.COMPANY
  AND TA.PAYGROUP    = TB.PAYGROUP
  AND TA.PAY_END_DT  = TB.PAY_END_DT
  AND TA.OFF_CYCLE   = TB.OFF_CYCLE
  AND TA.PAGE_NUM    = TB.PAGE_NUM
  AND TA.LINE_NUM    = TB.LINE_NUM
  AND TA.SEPCHK      = TB.SEPCHK
  AND TA.PAYCHECK_STATUS IN ('A', 'F', 'R')
  AND TB.TAX_CLASS_CAN IN ('CPP', 'QPP')
  AND TB.TAX_CUR     <> 0
  AND TB.TXGRS_CUR   <> 0
GROUP BY TB.PROVINCE, TB.WAGE_LOSS_PLAN

end-SELECT
end-procedure


!***********************************************
begin-procedure Update-Array-CPP-Max-ByTotal
!***********************************************

   let $Max_Out_ind = 'Y'
   let $CPP_MaxOut_Found = 'N'

   move 0 to #w
   while #w < #max_cpp_load and $CPP_MaxOut_Found = 'N'
     get #CPP_NLGrs_Totals from EE_CPP_Totals(#w) CPP_NLGrs_Totals

     if #CPP_NLGrs_Totals > #CPP_Max_Earns
       put $Max_Out_ind into EE_CPP_Totals(#w) MaxOut_Flag
       let $CPP_MaxOut_Found = 'Y'
     end-if

     add 1 to #w
   end-while
 end-procedure


!*********************************************
begin-procedure Update-Array-CPP-NLGrs-Adj
!*********************************************

   let #Accum_CPP_NLGrs_NonMax_Slips = 0
   let #Adjusted_CPP_NLGRs_Totals = 0
   let $Max_Out_Adjusted_Flag = 'N'

   move 0 to #w
   while #w < #max_cpp_load
     get #CPP_NLGrs_Totals $MaxOut_Flag from EE_CPP_Totals(#w) CPP_NLGrs_Totals MaxOut_Flag

     if $MaxOut_Flag = 'N'
       add #CPP_NLGrs_Totals to #Accum_CPP_NLGrs_NonMax_Slips
     end-if

     add 1 to #w
   end-while

   let #Adjusted_CPP_NLGRs_Totals = #CPP_Max_Earns - #Accum_CPP_NLGrs_NonMax_Slips

   move 0 to #w
   while #w < #max_cpp_load and $Max_Out_Adjusted_Flag = 'N'
     get $MaxOut_Flag from EE_CPP_Totals(#w) MaxOut_Flag

     if $MaxOut_Flag = 'Y'
       put #Adjusted_CPP_NLGRs_Totals into EE_CPP_Totals(#w) Adjusted_NLGrs_Totals
       let $Max_Out_Adjusted_Flag = 'Y'

     end-if

     add 1 to #w
   end-while

end-procedure


!******************************************
begin-procedure Find-CPP-Max-Out-Cheque
!******************************************

  let $CPP_MaxOut_Found = 'N'
  let $CPP_MaxOut_Prov  = ' '
  let $CPP_MaxOut_WLP   = ' '
  let #diff = 0

begin-SELECT
CA.PAY_END_DT
CA.CHECK_DT
CB.PROVINCE
CB.WAGE_LOSS_PLAN
CB.NLGRS_CUR
CB.TXGRS_CUR
CC.PAY_PDS_PER_YEAR

  if $CPP_MaxOut_Found = 'N'

    if &CB.NLGrs_Cur > &CB.TXGrs_Cur

      let #diff =  &CB.NLGrs_Cur - &CB.TXGrs_Cur

      if rtrim(&CB.Province, ' ') = 'QC'
        let #exempt_per_perid = #QPP_Exemption / &CC.Pay_Pds_Per_Year
      else
        let #exempt_per_perid = #CPP_Exemption / &CC.Pay_Pds_Per_Year
      end-if

      let #exempt_per_perid = trunc(#exempt_per_perid, 2)

      if #diff > #exempt_per_perid
        let $CPP_MaxOut_Found = 'Y'

        let $CPP_MaxOut_Prov = rtrim(&CB.Province, ' ')
        let $CPP_MaxOut_WLP  = rtrim(&CB.Wage_Loss_Plan, ' ')

        do Update-Array-CPP-Max-ByCheq
      end-if

    end-if
  end-if


FROM PS_PAY_CHECK    CA,
     PS_PAY_TAX_CAN  CB,
     PS_PAY_CALENDAR CC

WHERE CA.COMPANY     = &A.COMPANY
  AND CA.EMPLID      = $Emplid
  AND CA.COMPANY     = CC.COMPANY
  AND CA.PAYGROUP    = CC.PAYGROUP
  AND CA.PAY_END_DT  = CC.PAY_END_DT
  AND CC.CHECK_DT   >= $BeginDate
  AND CC.CHECK_DT   <= $AsOfDate
  AND CA.COMPANY     = CB.COMPANY
  AND CA.PAYGROUP    = CB.PAYGROUP
  AND CA.PAY_END_DT  = CB.PAY_END_DT
  AND CA.OFF_CYCLE   = CB.OFF_CYCLE
  AND CA.PAGE_NUM    = CB.PAGE_NUM
  AND CA.LINE_NUM    = CB.LINE_NUM
  AND CA.SEPCHK      = CB.SEPCHK
  AND CA.PAYCHECK_STATUS IN ('A', 'F', 'R')
  AND CB.TAX_CLASS_CAN IN ('CPP', 'QPP')
  AND CB.TAX_CUR     <> 0
  AND CB.TXGRS_CUR   <> 0
ORDER BY CA.PAY_END_DT DESC, CA.CHECK_DT DESC

end-SELECT
end-procedure


!*********************************************
begin-procedure Update-Array-CPP-Max-ByCheq
!*********************************************

  let $Prov_WLP_Found = 'N'
  move 0 to #w

  while #w < #max_cpp_load and $Prov_WLP_Found = 'N'
    get $E_Province $E_WLP from EE_CPP_Totals(#w) Province WLP

      if rtrim($E_Province, ' ') = $CPP_MaxOut_Prov and
         rtrim($E_WLP, ' ')      = $CPP_MaxOut_WLP

         put $Max_Out_ind into EE_CPP_Totals(#w) MaxOut_Flag
         let $Prov_WLP_Found = 'Y'
      end-if

    add 1 to #w
  end-while

end-procedure


!******************************************
begin-procedure Match-Prov-Totals-Arrays
!******************************************

  let $LoadY = 'Y'
  move 0 to #w

  while #w < #max_cpp_load
    get $E_Province $E_WLP #E_Adjusted_NLGrs_Totals #E_CPP_NLGrs_Totals from EE_CPP_Totals(#w) Province WLP Adjusted_NLGrs_Totals CPP_NLGrs_Totals

      if rtrim($E_Province, ' ') = rtrim($P_Province, ' ') and
         rtrim($E_WLP, ' ')      = rtrim($P_WLP, ' ')

         if #E_Adjusted_NLGrs_Totals > 0
           put #E_Adjusted_NLGrs_Totals $LoadY into Prov_Box_26(#p) P_Box26_Amt P_Update_Flag
         else
           put #E_CPP_NLGrs_Totals      $LoadY into Prov_Box_26(#p) P_Box26_Amt P_Update_Flag
         end-if

      end-if

    add 1 to #w
  end-while

end-procedure


!******************************************
begin-procedure CPP-NLGrs-Adj-with-Box14
!******************************************

  let $LoadN = 'N'
  let $LoadY = 'Y'
  let $Diff_Box14_Flag_Found = 'N'

  let #E_Adjusted_NLGrs_Totals = 0
  let #AdjustAmt = 0

  move 0 to #w

  while #w < #max_cpp_load
    get $E_Province $E_WLP #E_CPP_NLGrs_Totals from EE_CPP_Totals(#w) Province WLP CPP_NLGrs_Totals

    do Find-Box14-Amount

    if #Box_14_Amt > 0
      put #Box_14_Amt into EE_CPP_Totals(#w) Box_14_Amt

      if #E_CPP_NLGrs_Totals = #Box_14_Amt
        put $LoadN into EE_CPP_Totals(#w) CPP_NLGrs_Diff_Box14_Flag

      else
        put $LoadY into EE_CPP_Totals(#w) CPP_NLGrs_Diff_Box14_Flag

      end-if

    end-if

    add 1 to #w
  end-while


  let #Adjusted_CPP_NLGRs_Totals = #CPP_Max_Earns - #CPP_Contrib_NLGrs_Total

  do Sort-EE-CPP-Array

  move 0 to #w

  while #w < #max_cpp_load and #Adjusted_CPP_NLGRs_Totals > 0
    get $Diff_Box14_Flag #CPP_NLGrs_Totals #Box_14_Amt from EE_CPP_Totals(#w) CPP_NLGrs_Diff_Box14_Flag CPP_NLGrs_Totals Box_14_Amt

    if $Diff_Box14_Flag = 'Y'

      if #CPP_NLGrs_Totals + #Adjusted_CPP_NLGRs_Totals >=  #Box_14_Amt

        let #AdjustAmt = #Box_14_Amt - #CPP_NLGrs_Totals
        put #Box_14_Amt into EE_CPP_Totals(#w) Adjusted_NLGrs_Totals

        let #Adjusted_CPP_NLGRs_Totals = #Adjusted_CPP_NLGRs_Totals - #AdjustAmt

      else

        if #Adjusted_CPP_NLGRs_Totals > 0

          let #E_Adjusted_NLGrs_Totals = #CPP_NLGrs_Totals + #Adjusted_CPP_NLGRs_Totals
          put #E_Adjusted_NLGrs_Totals into EE_CPP_Totals(#w) Adjusted_NLGrs_Totals

          let #Adjusted_CPP_NLGRs_Totals = 0

        end-if

      end-if
    end-if

    add 1 to #w

  end-while

end-procedure


!***********************************
begin-procedure Find-Box14-Amount
!***********************************

  let #Box_14_Amt = 0

begin-SELECT
BA.CAN_YE_BOX_AMT



  move &BA.CAN_YE_BOX_AMT to #Box_14_Amt


 FROM PS_CAN_YE_DETAIL  BA
WHERE BA.COMPANY         = &A.COMPANY
  AND BA.EMPLID          = $Emplid
  AND BA.CALENDAR_YEAR   = #CalendarYear
  AND BA.SEQUENCE_NUMBER = 0
  AND BA.TAXFORM_ID      = 'T'
  AND BA.WAGE_LOSS_PLAN  = $E_WLP
  AND BA.PROVINCE        = $E_Province
  AND BA.BOX             = '14'

end-SELECT
end-procedure


!***********************************
begin-procedure Sort-EE-CPP-Array
!***********************************

 move 0 to #sorted
   while not #sorted
      move 0 to #moves
      move 0 to #low_idx
      while #low_idx < #max_cpp_load
         let #high_idx = #low_idx + 1
         get $Low_Province $Low_WLP #Low_CPP_NLGrs_Totals #Low_CPP_TxGrs_Totals
             #Low_CPP_Tax_Totals #Low_Adjusted_NLGrs_Totals $Low_MaxOut_Flag #Low_Box_14_Amt
             $Low_CPP_NLGrs_Diff_Box14_Flag
            from EE_CPP_Totals (#low_idx) Province WLP CPP_NLGrs_Totals
                                          CPP_TxGrs_Totals CPP_Tax_Totals
                                          Adjusted_NLGrs_Totals MaxOut_Flag
                                          Box_14_Amt CPP_NLGrs_Diff_Box14_Flag

         get $High_Province $High_WLP #High_CPP_NLGrs_Totals #High_CPP_TxGrs_Totals
             #High_CPP_Tax_Totals #High_Adjusted_NLGrs_Totals $High_MaxOut_Flag #High_Box_14_Amt
             $High_CPP_NLGrs_Diff_Box14_Flag
            from EE_CPP_Totals (#high_idx) Province WLP CPP_NLGrs_Totals
                                           CPP_TxGrs_Totals CPP_Tax_Totals
                                           Adjusted_NLGrs_Totals MaxOut_Flag
                                           Box_14_Amt CPP_NLGrs_Diff_Box14_Flag

         if #Low_CPP_Tax_Totals < #High_CPP_Tax_Totals
            put $Low_Province $Low_WLP #Low_CPP_NLGrs_Totals #Low_CPP_TxGrs_Totals
                #Low_CPP_Tax_Totals #Low_Adjusted_NLGrs_Totals $Low_MaxOut_Flag #Low_Box_14_Amt
                $Low_CPP_NLGrs_Diff_Box14_Flag
            into EE_CPP_Totals (#high_idx) Province WLP CPP_NLGrs_Totals
                                           CPP_TxGrs_Totals CPP_Tax_Totals
                                           Adjusted_NLGrs_Totals MaxOut_Flag
                                           Box_14_Amt CPP_NLGrs_Diff_Box14_Flag

            put $High_Province $High_WLP #High_CPP_NLGrs_Totals #High_CPP_TxGrs_Totals
                #High_CPP_Tax_Totals #High_Adjusted_NLGrs_Totals $High_MaxOut_Flag #High_Box_14_Amt
                $High_CPP_NLGrs_Diff_Box14_Flag
            into EE_CPP_Totals (#low_idx) Province WLP CPP_NLGrs_Totals
                                          CPP_TxGrs_Totals CPP_Tax_Totals
                                          Adjusted_NLGrs_Totals MaxOut_Flag
                                          Box_14_Amt CPP_NLGrs_Diff_Box14_Flag
            add 1 to #moves
          end-if
          add 1 to #low_idx
      end-while
      if #moves = 0
         move 1 to #sorted
      end-if
   end-while

end-procedure

!***********************************
begin-procedure Capture-Earns-Date
!***********************************

Begin-select
EARN.EARNS_END_DT

     let $EarnsDate = &EARN.EARNS_END_DT
     move 'Y' to $EarnsFound

FROM PS_PAY_EARNINGS EARN, 
     PS_PAY_OTH_EARNS OTH
WHERE EARN.EMPLID = $Emplid
  AND EARN.COMPANY = OTH.COMPANY
  AND EARN.PAYGROUP = OTH.PAYGROUP
  AND EARN.PAY_END_DT = OTH.PAY_END_DT
  AND EARN.OFF_CYCLE = OTH.OFF_CYCLE
  AND EARN.PAGE_NUM = OTH.PAGE_NUM
  AND EARN.LINE_NUM = OTH.LINE_NUM
  AND EARN.ADDL_NBR = OTH.ADDL_NBR
  AND EARN.PAY_END_DT >= $BeginDate
  AND EARN.PAY_END_DT <= $AsOfDate
  AND OTH.ERNCD = $ErnCd
  ORDER BY EARN.EARNS_END_DT DESC
end-select

end-procedure

