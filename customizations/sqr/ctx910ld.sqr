!**************************************
begin-procedure Status-CPP
!**************************************

 let #Interim_CPP  = 0
 let #Interim_Total_CPP  = 0
 let #Row_CPP = 0

begin-select
TAXCPP.TAX_CLASS_CAN
TAXCPP.NLGRS_CUR
TAXCPP.PROVINCE
TAXCPP.WAGE_LOSS_PLAN
DTACPP.PCT_EXEMPT
DTACPP.USE_FOR_CPP
CHKCPP.CHECK_DT
CALCPP.CHECK_DT

     if  &DTACPP.USE_FOR_CPP = 'Y'
         let #Row_CPP = Round((&TAXCPP.NLGRS_CUR * ((100 - &DTACPP.PCT_Exempt) / 100)),2)
     else
         let #Row_CPP =  Round(&TAXCPP.NLGRS_CUR,2)
     end-if
     if  rtrim(&TAXCPP.PROVINCE, ' ') = $Province and
         rtrim(&TAXCPP.WAGE_LOSS_PLAN, ' ') = $WageLossPlan

         if  #Interim_Total_CPP >= #CPP_Max_Earns
             add 0 to #Interim_CPP
         else
             if Round((#Interim_Total_CPP + #Row_CPP),2) >=  #CPP_Max_Earns
                 let #Interim_CPP = #Interim_CPP + Round ((#CPP_Max_Earns - #Interim_Total_CPP),2)
             else
                 let #Interim_CPP = #Interim_CPP + Round (#Row_CPP,2)
             end-if
          end-if
      end-if

 add #Row_CPP to #Interim_Total_CPP
 if #Interim_Total_CPP >= #CPP_Max_Earns
    move #CPP_Max_Earns to #Interim_Total_CPP
 end-if

FROM PS_PAY_CHECK    CHKCPP,
     PS_PAY_TAX_CAN  TAXCPP,
     PS_CAN_TAX_DATA DTACPP,
     PS_PAY_CALENDAR CALCPP
WHERE CHKCPP.EMPLID      = $Emplid
AND   CALCPP.COMPANY     = CHKCPP.COMPANY
