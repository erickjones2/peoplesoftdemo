begin-procedure Find-EE-Ern-Ded-Bal
!**************************************

  move ' ' to $EE_Ern_Fnd

begin-SELECT DISTINCT

EB.EMPLID           () on-break print=never after=EE-Change
EB.WAGE_LOSS_PLAN
EB.PROVINCE
WP5.CANADA_BN

  move &EB.Emplid          to $EMPLID
