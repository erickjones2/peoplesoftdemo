  INSERT INTO PS_CAN_YE_SLIP (COMPANY,
                              EMPLID,
                              CALENDAR_YEAR,
                              TAXFORM_ID,
                              WAGE_LOSS_PLAN,
                              PROVINCE,
                              SEQUENCE_NUMBER,
                              CAN_YE_SLIP_SEQ,
                              FOOTNOTE_COUNT,
                              REPORTING_ID,
                              UI_EXEMPT,
                              PROCESS_FLAG,
                              RELEVE_SLIP_NO,
                              CIT_TO_T4A_GRS,
                              CIT_TO_T4A_TAX,
                              RELEVE2_SLIP_NO,
                              PROVENANCE,
                              PROVENANCE2,
                              EMPLOYMENT_CODE,
                              SIN_CONTRB_SPOUSE,
                              NAME_CONTRB_SPOUSE,
                              YE_SLIP_PROCESS)
                      VALUES (&AXA.COMPANY,
                              &AXA.EMPLID,
                              &AXA.CALENDAR_YEAR,
                              &AXA.TAXFORM_ID,
                              &AXA.WAGE_LOSS_PLAN,
                              &AXA.PROVINCE,
                              &AXA.SEQUENCE_NUMBER,
                              #Temp-Slip-Seq,
                              &AXB.FOOTNOTE_COUNT,
                              &AXB.REPORTING_ID,
                              &AXB.UI_EXEMPT,
                              &AXB.PROCESS_FLAG,
                              &AXB.RELEVE_SLIP_NO,
                              &AXB.CIT_TO_T4A_GRS,
                              &AXB.CIT_TO_T4A_TAX,
                              &AXB.RELEVE2_SLIP_NO,
                              'RPA',
                              &AXB.PROVENANCE2,
                              ' ',
                              ' ',
                              ' ',
                              'O');
end-SQL

end-procedure


