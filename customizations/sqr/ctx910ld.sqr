!**************************************


begin-SELECT
AXB.CAN_YE_SLIP_SEQ
AXB.FOOTNOTE_COUNT
AXB.REPORTING_ID
AXB.UI_EXEMPT
AXB.PROCESS_FLAG
AXB.RELEVE_SLIP_NO
AXB.CIT_TO_T4A_GRS
AXB.CIT_TO_T4A_TAX
AXB.RELEVE2_SLIP_NO
AXB.PROVENANCE
AXB.PROVENANCE2

FROM  PS_CAN_YE_SLIP AXB
