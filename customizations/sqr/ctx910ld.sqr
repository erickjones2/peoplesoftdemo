!****************************************
begin-procedure Split-RL2-Box-A
!****************************************

! If a Box A amount has been found, we need to change the
! Provenance value from "RO" to "RPA".
! If the ONLY Box on the RL2 slip is Box "A" then we can
! update the value in place. If there are other boxes,
! then we have to add a new sequence number and detail records.


  let #AXA-Sequence_Number = 99999
  let $AXA-Wage_Loss_Plan = ' '
  let $AXA-EMPLID = ' '
  let #AXA-Can_YE_Slip_Seq = 99999

begin-SELECT
AXA.COMPANY
AXA.EMPLID
AXA.CALENDAR_YEAR
AXA.SEQUENCE_NUMBER
AXA.TAXFORM_ID
AXA.WAGE_LOSS_PLAN
AXA.PROVINCE
AXA.CAN_YE_SLIP_SEQ
AXA.BOX
AXA.CAN_YE_BOX_TEXT
AXA.CAN_YE_BOX_AMT

  if #AXA-Sequence_Number <> &AXA.SEQUENCE_NUMBER or
     $AXA-Wage_Loss_Plan <> rtrim(&AXA.WAGE_LOSS_PLAN, ' ') or
     $AXA-EMPLID <> rtrim(&AXA.EMPLID, ' ') or
     #AXA-Can_YE_Slip_Seq <> &AXA.CAN_YE_SLIP_SEQ

    do Set-New-Can-YE-Slip
  end-if

  do Set-New-Can-YE-Detail
  do Delete-Old-Can-YE-Detail

  let #AXA-Sequence_Number = &AXA.SEQUENCE_NUMBER
  let $AXA-Wage_Loss_Plan  = rtrim(&AXA.WAGE_LOSS_PLAN, ' ')
  let $AXA-EMPLID = rtrim(&AXA.EMPLID, ' ')
  let #AXA-Can_YE_Slip_Seq = &AXA.CAN_YE_SLIP_SEQ

FROM  PS_CAN_YE_DETAIL AXA
WHERE AXA.COMPANY       = $Company
  AND AXA.CALENDAR_YEAR = #CalendarYear
  AND AXA.TAXFORM_ID    = 'V'
  AND AXA.PROVINCE      = 'QC'
  AND AXA.BOX           = 'A'
  AND EXISTS (SELECT 'X'
              FROM PS_CAN_YE_DETAIL AXA1
              WHERE AXA1.COMPANY         = AXA.COMPANY
                AND AXA1.EMPLID          = AXA.EMPLID
                AND AXA1.CALENDAR_YEAR   = AXA.CALENDAR_YEAR
                AND AXA1.SEQUENCE_NUMBER = AXA.SEQUENCE_NUMBER
                AND AXA1.TAXFORM_ID      = AXA.TAXFORM_ID
                AND AXA1.WAGE_LOSS_PLAN  = AXA.WAGE_LOSS_PLAN
                AND AXA1.PROVINCE        = AXA.PROVINCE
                AND AXA1.CAN_YE_SLIP_SEQ = AXA.CAN_YE_SLIP_SEQ
                AND AXA1.BOX             <> 'A'
                AND AXA1.BOX             <> 'J')
ORDER BY AXA.EMPLID,AXA.SEQUENCE_NUMBER ASC, WAGE_LOSS_PLAN ASC, CAN_YE_SLIP_SEQ ASC
end-SELECT


begin-SELECT
AXC.COMPANY
AXC.EMPLID
AXC.CALENDAR_YEAR
AXC.SEQUENCE_NUMBER
AXC.TAXFORM_ID
AXC.WAGE_LOSS_PLAN
AXC.PROVINCE
AXC.CAN_YE_SLIP_SEQ

  DO Update-Box-A-Only-RL2S

FROM  PS_CAN_YE_SLIP AXC
WHERE AXC.COMPANY         = $Company
  AND AXC.CALENDAR_YEAR   = #CalendarYear
  AND AXC.TAXFORM_ID      = 'V'
  AND AXC.PROVINCE        = 'QC'
  AND AXC.PROVENANCE      = 'RO'
  AND EXISTS (SELECT 'X'
              FROM PS_CAN_YE_DETAIL AXC1
              WHERE AXC1.COMPANY         = AXC.COMPANY
                AND AXC1.EMPLID          = AXC.EMPLID
                AND AXC1.CALENDAR_YEAR   = AXC.CALENDAR_YEAR
                AND AXC1.SEQUENCE_NUMBER = AXC.SEQUENCE_NUMBER
                AND AXC1.TAXFORM_ID      = AXC.TAXFORM_ID
                AND AXC1.WAGE_LOSS_PLAN  = AXC.WAGE_LOSS_PLAN
                AND AXC1.PROVINCE        = AXC.PROVINCE
                AND AXC1.CAN_YE_SLIP_SEQ = AXC.CAN_YE_SLIP_SEQ
                AND AXC1.BOX             = 'A')
  AND NOT EXISTS (SELECT 'X'
                  FROM PS_CAN_YE_DETAIL AXC2
                  WHERE AXC2.COMPANY         = AXC.COMPANY
                    AND AXC2.EMPLID          = AXC.EMPLID
                    AND AXC2.CALENDAR_YEAR   = AXC.CALENDAR_YEAR
                    AND AXC2.SEQUENCE_NUMBER = AXC.SEQUENCE_NUMBER
                    AND AXC2.TAXFORM_ID      = AXC.TAXFORM_ID
                    AND AXC2.WAGE_LOSS_PLAN  = AXC.WAGE_LOSS_PLAN
                    AND AXC2.PROVINCE        = AXC.PROVINCE
                    AND AXC2.CAN_YE_SLIP_SEQ = AXC.CAN_YE_SLIP_SEQ
                    AND AXC2.BOX             <> 'A'
                    AND AXC2.BOX             <> 'J')
end-SELECT



end-procedure


!**************************************
