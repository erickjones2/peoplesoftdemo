end-procedure
!
!**************************************************************************
!
! Procedure: Get_Payment_Summ
!
! Description: Get and print the pay summary details for the payee.
!
!**************************************************************************
begin-procedure Get_Payment_Summ
begin-SELECT on-error=SQL-Error
C.GPCN_PIN_AC1_NUM                &GPCN_PIN_AC1_NUM
C.GPCN_PIN_AC2_NUM                &GPCN_PIN_AC2_NUM
C.GPCN_PIN_AC3_NUM                &GPCN_PIN_AC3_NUM
C.GPCN_PIN_AC4_NUM                &GPCN_PIN_AC4_NUM
C.GPCN_PIN_AC5_NUM                &GPCN_PIN_AC5_NUM
C.GPCN_PIN_AC6_NUM                &GPCN_PIN_AC6_NUM
C.DESCR                           &C.DESCR
C.SEQ_NUM
A.ORIG_CAL_RUN_ID                 &ORIG_CAL_ID
A.RUN_TYPE                        &RUN_TYP
A.PRD_BGN_DT                      &PRD_BGNDT
A.PRD_END_DT                      &PRD_ENDDT
        let #Calc_Rslt_Val = 0
        let #Pin_Num = &GPCN_PIN_AC1_NUM
        do Get_Accum_Values
        let #Calc_Acum_Val1 = #Calc_Rslt_Val

        let #Calc_Rslt_Val = 0

        let #Pin_Num = &GPCN_PIN_AC2_NUM
        do Get_Accum_Values
        let #Calc_Acum_Val2 = #Calc_Rslt_Val

        let #Calc_Rslt_Val = 0

        let #Pin_Num = &GPCN_PIN_AC3_NUM
        do Get_Accum_Values
        let #Calc_Acum_Val3 = #Calc_Rslt_Val

        let #Calc_Rslt_Val = 0

        let #Pin_Num = &GPCN_PIN_AC4_NUM
        do Get_Accum_Values
        let #Calc_Acum_Val4 = #Calc_Rslt_Val

        let #Calc_Rslt_Val = 0

        let #Pin_Num = &GPCN_PIN_AC5_NUM
        do Get_Accum_Values
        let #Calc_Acum_Val5 = #Calc_Rslt_Val

        let #Calc_Rslt_Val = 0

        let #Pin_Num = &GPCN_PIN_AC6_NUM
        do Get_Accum_Values
        let #Calc_Acum_Val6 = #Calc_Rslt_Val

        let #PSAC_SEQ_NUM = &C.SEQ_NUM
        If $curr_language_cd <> 'ENG'
           do Get_Trans_Payment_Summ
              If rtrim($Pay_Summ_Desc, ' ') = ''
                 let $Pay_Summ_Desc  = &C.DESCR
              End-if
        Else
           let $Pay_Summ_Desc  = &C.DESCR
        End-if

        !if #Calc_Acum_Val1 <> 0 or
        !      #Calc_Acum_Val2 <> 0 or
        !      #Calc_Acum_Val3 <> 0 or
        !      #Calc_Acum_Val4 <> 0 or
        !      #Calc_Acum_Val5 <> 0 or
        !      #Calc_Acum_Val6 <> 0
        Let $ORIG_CAL_ID=&ORIG_CAL_ID
        Let $PRD_BGN_DT = &PRD_BGNDT
        Let $PRD_END_DT = &PRD_ENDDT
        if $Pay_Summ_Desc = 'Current Period'
            Let #MP_GROSS = #Calc_Acum_Val1
            If strtodate($SegBgnDt,'dd-Mon-yyyy') > strtodate('01-Jan-2019','dd-Mon-yyyy')
            Let #MP_NET = #Calc_Acum_Val2
            Else
            Let #MP_NET = #Calc_Acum_Val5
            End-If
            Let $RUN_TYP = &RUN_TYP
        End-if
           print $Pay_Summ_Desc (+1, {colA}) Bold

           if #Calc_Acum_Val1 <> 0 or (#Calc_Acum_Val1 = 0 and rtrim(ltrim($Acum_Lbl1, ' '), ' ') <>'' )
              print #Calc_Acum_Val1 (0, {colAC1})  edit '99999999.99'
           End-if
           if #Calc_Acum_Val2 <> 0 or (#Calc_Acum_Val2 = 0 and rtrim(ltrim($Acum_Lbl2, ' '), ' ') <>'' )
              print #Calc_Acum_Val2 (0, {colAC2})  edit '99999999.99'
           End-if
           if #Calc_Acum_Val3 <> 0 or (#Calc_Acum_Val3 = 0 and rtrim(ltrim($Acum_Lbl3, ' '), ' ') <>'' )
              print #Calc_Acum_Val3 (0, {colAC3})  edit '99999999.99'
           End-if
           if #Calc_Acum_Val4 <> 0 or (#Calc_Acum_Val4 = 0 and rtrim(ltrim($Acum_Lbl4, ' '), ' ') <>'' )
              print #Calc_Acum_Val4 (0, {colAC4})  edit '99999999.99'
           End-if
           if #Calc_Acum_Val5 <> 0 or (#Calc_Acum_Val5 = 0 and rtrim(ltrim($Acum_Lbl5, ' '), ' ') <>'' )
              print #Calc_Acum_Val5 (0, {colAC5})  edit '99999999.99'
           End-if
           if #Calc_Acum_Val6 <> 0 or (#Calc_Acum_Val6 = 0 and rtrim(ltrim($Acum_Lbl6, ' '), ' ') <>'' )
              print #Calc_Acum_Val6 (0, {colAC6})  edit '99999999.99'
           End-if
       !End-if

FROM PS_GPCN_PSLP_ACUM C,
     PS_GPCN_PYGRP_PSLP D,
     PS_GP_PYE_SEG_STAT B,
     PS_GP_PYE_PRC_STAT A
WHERE C.GPCN_PAYSLIP_ID = D.GPCN_PAYSLIP_ID
  AND D.GP_PAYGROUP     = B.GP_PAYGROUP
  AND D.GPCN_PAYSLIP_ID = $payslip_id
  AND B.CAL_RUN_ID      = A.CAL_RUN_ID
  AND A.ORIG_CAL_RUN_ID = B.ORIG_CAL_RUN_ID
  AND B.EMPLID          = A.EMPLID
  AND B.EMPL_RCD        = A.EMPL_RCD
  AND B.CAL_ID          = A.CAL_ID
  AND B.GP_PAYGROUP     = A.GP_PAYGROUP
  AND A.EMPLID          = $Emplid
  AND A.EMPL_RCD        = #Empl_Rcd
  AND A.CAL_RUN_ID      = $Cal_Run_ID
  AND A.CAL_ID          = $Cal_ID
  AND A.GP_PAYGROUP     = $GP_Paygroup
  AND B.RSLT_SEG_NUM    = #Rslt_Seg_num
  AND B.RSLT_REV_NUM = (SELECT MAX (SEG1.RSLT_REV_NUM)
                             FROM PS_GP_PYE_SEG_STAT SEG1
                              WHERE B.EMPLID       = SEG1.EMPLID
                                AND B.CAL_RUN_ID   = SEG1.CAL_RUN_ID
                                AND B.EMPL_RCD     = SEG1.EMPL_RCD
                                AND B.GP_PAYGROUP  = SEG1.GP_PAYGROUP
                                AND B.CAL_ID       = SEG1.CAL_ID
                                AND B.RSLT_SEG_NUM = SEG1.RSLT_SEG_NUM)
  AND C.EFFDT = (SELECT MAX(C1.EFFDT) FROM PS_GPCN_PSLP_ACUM C1
                          WHERE C1.GPCN_PAYSLIP_ID = C.GPCN_PAYSLIP_ID
                            AND C1.EFFDT <=$SegEndDt)
  AND D.EFFDT = (SELECT MAX(D1.EFFDT) FROM PS_GPCN_PYGRP_PSLP D1
                          WHERE D1.GPCN_PAYSLIP_ID = D.GPCN_PAYSLIP_ID
                            AND D1.GP_PAYGROUP = D.GP_PAYGROUP
                            AND D1.EFFDT <=$SegEndDt)
ORDER BY C.SEQ_NUM
end-select

end-procedure
!
!**************************************************************************
!
! Procedure: Get_Trans_Payment_Summ
!
! Description: Get and print the pay summary details for the payee.
!
!**************************************************************************
