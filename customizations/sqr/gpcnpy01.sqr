     Let $WhereClause = ' AND JOB.SETID_LOCATION = ' || '''' || &Setid1 || '''' || ' AND JOB.LOCATION = ' || '''' || &Location || ''''
     Show 'Processing Location : ' &Location
     Do Process-Payee
   End-If
    FROM PS_GPCN_PSLP_LC_RC CPY1
