!
!**************************************************************************
!
! Procedure: Get_Disburs_Banked
!
! Description: Get and print the banking details for the payee.
!
!**************************************************************************
begin-procedure Get_Disburs_Banked

        let $First_Disb = 'Y'

begin-SELECT on-error=SQL-Error
B.GP_PMT_AMT
D.BRANCH_EC_CD
D.ACCOUNT_EC_ID
E.BANK_NM
BRNCH.DESCR

    If #Current-line >  80
       NEW-PAGE
       do Print_Heading
    End-if

    If $First_Disb = 'Y'
       do Print_Disb_headings
       let $First_Disb = 'N'
    end-if

    let $Bank_Name     = &E.BANK_NM
    let $BSB           = &D.BRANCH_EC_CD
    let $Branch_Name   = &BRNCH.DESCR

    print $Bank_Name (+2, {ColA})

    print $Branch_Name (0, {ColC1})

    let $Acct_Nbr = &D.ACCOUNT_EC_ID
    print $Acct_Nbr (0, {ColG1})

    let #Paymnt_Amt = &B.GP_PMT_AMT
    print #Paymnt_Amt (0, {ColN}) edit 999999999.99

FROM PS_GP_CALENDAR A
, PS_GP_PAYMENT B
, PS_GP_NET_DIST_DTL C
, PS_PYE_BANKACCT D
, PS_BANK_EC_TBL E
, PS_BANK_BRANCH_TBL BRNCH
  WHERE B.CAL_ID       = A.CAL_ID
    AND B.GP_PAYGROUP  = A.GP_PAYGROUP
    AND A.RUN_TYPE     = C.RUN_TYPE
    AND B.EMPLID       = C.EMPLID
    AND C.EMPLID       = D.EMPLID
    AND B.EMPL_RCD     = C.EMPL_RCD
    AND B.INSTANCE     = C.INSTANCE
    AND C.ACCOUNT_ID   = D.ACCOUNT_ID
    AND D.COUNTRY_CD   = E.COUNTRY_CD
    AND D.BANK_CD      = E.BANK_CD
    AND D.COUNTRY_CD   = BRNCH.COUNTRY_CD
    AND D.BANK_CD      = BRNCH.BANK_CD
    AND D.BRANCH_EC_CD = BRNCH.BRANCH_EC_CD
    AND B.PMT_TYPE     = '01'
    AND C.EFFDT =(SELECT MAX(C1.EFFDT) FROM PS_GP_NET_DIST_DTL C1 WHERE C1.EMPLID = C.EMPLID
                      AND C1.EMPL_RCD = C.EMPL_RCD
                      AND C1.RUN_TYPE = C.RUN_TYPE
                      AND C1.INSTANCE = C.INSTANCE)
    AND B.EMPLID        = $Emplid
    AND B.EMPL_RCD      = #Empl_Rcd
    AND B.CAL_RUN_ID    = $Cal_Run_ID
    AND B.GP_PAYGROUP   = $GP_Paygroup
    AND B.CAL_ID        = $Cal_ID

end-select
end-procedure
!**************************************************************************
!
! Procedure: Get_Disburs_Nonbanked
!
! Description: Get and print the non banking disburment details for the payee.
!
!**************************************************************************
begin-procedure Get_Disburs_Nonbanked

begin-SELECT on-error=SQL-Error
PMNT.PAYMENT_MTHD    &PMNT_Mthd
SUM(PMNT.GP_PMT_AMT) &NetPayAmt

    If #Current-line >  75
       NEW-PAGE
       do Print_Heading
    End-if

    If $First_Disb = 'Y'
       do Print_Disb_headings
       let $First_Disb = 'N'
    end-if

    let #Paymnt_Amt = &NetPayAmt
    if #Paymnt_Amt <> 0
       Let $FieldName  = 'PAYMENT_MTHD'
       Let $FieldValue = &PMNT_Mthd

       !Let $curr_language_cd = 'ENG'

       Do Read-Translate-Table
       Print $XlatLongName (+2,{ColA})
       print #Paymnt_Amt   (0, {ColN}) edit 999999999.99
    end-if

FROM PS_GP_PAYMENT PMNT
    WHERE PMNT.EMPLID      = $Emplid
      AND PMNT.EMPL_RCD    = #Empl_Rcd
      AND PMNT.CAL_RUN_ID  = $Cal_Run_ID
      AND PMNT.CAL_ID      = $Cal_ID
      AND PMNT.GP_PAYGROUP = $GP_Paygroup
      AND PMNT.PAYMENT_MTHD <> 'T'
      AND PMNT.PMT_TYPE    = '01'
GROUP BY PMNT.PAYMENT_MTHD
end-select
end-procedure

!**************************************************************************
!
! Procedure: Get_Messages
!
! Description: Get and print the messages for the payee.
!
!**************************************************************************
begin-procedure Get_Messages

        Let $First_Msg = 'Y'
        Let #Msg_Nbr = 0

begin-SELECT on-error=SQL-Error
B.GPCN_MSG_NBR
A.GPCN_MESSAGE

        If #Current-line > 75
           NEW-PAGE
           do Print_Heading
        End-if

        If $First_Msg = 'Y'
            print 'Messages' (+2) Bold CENTER
            Let #Mesg_StartLine = #Current-Line
            let $First_Msg = 'N'
        end-if

        let #Msg_Nbr = &B.GPCN_MSG_NBR
        let $Message = &A.GPCN_MESSAGE

        if #Msg_nbr <> 0
           print #Msg_Nbr (+1, {ColC}) EDIT '9999'
           print $Message (0, 25) Wrap {colK} 3
        end-if

FROM PS_GPCN_PSLP_MSG A
   , PS_GPCN_PSLP_RSLT B
WHERE A.CAL_RUN_ID = B.CAL_RUN_ID
    AND A.GPCN_MSG_NBR  = B.GPCN_MSG_NBR
    AND B.EMPLID        = $Emplid
    AND B.EMPL_RCD      = #Empl_Rcd
    AND B.CAL_RUN_ID    = $Cal_Run_ID
    AND B.GP_PAYGROUP   = $GP_Paygroup
    AND B.CAL_ID        = $Cal_ID
end-select
end-procedure

!**************************************************************************
!
! Procedure: Print_ED_Headings
!
! Description: Prints the section headings
!
!**************************************************************************
begin-procedure Print_ED_Headings

        print $Sect_Hdg (+3,) Center Bold Underline

        print $Description_Lbl (+1, {ColA}) Bold

!!       'Current Values' Sub-Headings

        Do Find-Start-Pos ($Base_Lbl, {colED3},#ReturnCol)
        Let #ReturnCol = #ReturnCol - 3
        print $Base_Lbl (0, #ReturnCol) Bold

        Do Find-Start-Pos ($Percent_Lbl, {colED5},#ReturnCol)
        Let #ReturnCol = #ReturnCol - 3
        print $Percent_Lbl (0, #ReturnCol) Bold

        Do Find-Start-Pos ($Rate_Lbl, {colED6},#ReturnCol)
        Let #ReturnCol = #ReturnCol - 3
        print $Rate_Lbl (0, #ReturnCol) Bold

        Do Find-Start-Pos ($Units_Lbl, {colED7},#ReturnCol)
        Let #ReturnCol = #ReturnCol - 3
        print $Units_Lbl (0, #ReturnCol) Bold

        Do Find-Start-Pos ($Amount_Lbl, {colED8},#ReturnCol)
        Let #ReturnCol = #ReturnCol - 3
        print $Amount_Lbl (0, #ReturnCol) Bold


!!       'Payback' Sub-Headings

        Do Find-Start-Pos ($Amount_Lbl, {colED9},#ReturnCol)
        Let #ReturnCol = #ReturnCol - 3
        print $Amount_Lbl (0, #ReturnCol) Bold


!!       'Adjustments' Sub-Headings

        Do Find-Start-Pos ($Amount_Lbl, {colED10},#ReturnCol)
        Let #ReturnCol = #ReturnCol - 3
        print $Amount_Lbl (0, #ReturnCol) Bold

        Do Find-Start-Pos ($Base_Lbl, {colED11},#ReturnCol)
        Let #ReturnCol = #ReturnCol - 3
        print $Base_Lbl (0, #ReturnCol) Bold

        Do Find-Start-Pos ($Units_Lbl, {colED12},#ReturnCol)
        Let #ReturnCol = #ReturnCol - 3
        print $Units_Lbl (0, #ReturnCol) Bold


!!       'YTD Values' Sub-Headings

        Do Find-Start-Pos ($Units_Lbl, {colED13},#ReturnCol)
        Let #ReturnCol = #ReturnCol - 3
        print $Units_Lbl (0, #ReturnCol) Bold

        Do Find-Start-Pos ($Amount_Lbl, {colED14},#ReturnCol)
        If $curr_language_cd <> 'ENG'
           Let #ReturnCol = #ReturnCol - 6
        Else
           Let #ReturnCol = #ReturnCol - 3
        End-if
        print $Amount_Lbl (0, #ReturnCol) Bold

end-procedure

