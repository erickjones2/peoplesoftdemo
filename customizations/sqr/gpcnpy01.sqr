  End-If
  let $sort_key1        = &RC.GPCN_SORT_KEY1
  let $sort_key2        = &RC.GPCN_SORT_KEY2
  let $sort_key3        = &RC.GPCN_SORT_KEY3

end-procedure
!
!**************************************************************************
! Procedure: Get-JOB-JOBTitle-Details
!            Get the JOB Code title
!**************************************************************************
Begin-procedure Get-JOB-JOBTitle-Details
  Let $JobTitle = ''
Begin-SELECT
JDD3.DESCR

     Let $JobTitle = &JDD3.DESCR

  FROM PS_JOBCODE_TBL JDD3
    WHERE JDD3.SETID = $SetIDJobCode
      AND JDD3.JOBCODE = $JobCode
      AND JDD3.EFFDT = (SELECT MAX(JDD31.EFFDT) FROM PS_JOBCODE_TBL JDD31
                             WHERE JDD3.SETID = JDD31.SETID
                               AND JDD3.JOBCODE = JDD31.JOBCODE
                               AND JDD31.EFFDT <= $SegEndDt)
End-SELECT
End-Procedure
!
!**************************************************************************
! Procedure: Get-JOB-Trans-JOBTitle-Details
!            Get the JOB Code title
!**************************************************************************
Begin-procedure Get-JOB-Trans-JOBTitle-Details
  Let $JobTitle = ''
Begin-SELECT
JDD3LNG.DESCR

     Let $JobTitle = &JDD3LNG.DESCR

  FROM PS_JOBCODE_LANG JDD3LNG
    WHERE JDD3LNG.SETID = $SetIDJobCode
      AND JDD3LNG.JOBCODE = $JobCode
      AND JDD3LNG.EFFDT = (SELECT MAX(JDD3LNG1.EFFDT) FROM PS_JOBCODE_LANG JDD3LNG1
                             WHERE JDD3LNG.SETID = JDD3LNG1.SETID
                               AND JDD3LNG.JOBCODE = JDD3LNG1.JOBCODE
                               AND JDD3LNG1.EFFDT <= $SegEndDt)
      AND JDD3LNG.LANGUAGE_CD = $curr_language_cd
End-SELECT
End-Procedure
