!**************************************************************************
!
! Procedure: Get-Citizen_ID
! Description: Get Citizen ID for employee = NID from National ID table
!**************************************************************************
begin-procedure Get_Citizen_ID
 Let $Citizen_ID = ''
Begin-SELECT
NID.NATIONAL_ID
    Let $Citizen_ID = &NID.NATIONAL_ID
   FROM PS_PERS_NID NID
    WHERE NID.EMPLID  = $Emplid
      AND NID.NATIONAL_ID_TYPE = 'NID'
      AND NID.COUNTRY = 'CHN'
End-SELECT
end-procedure
!
!**************************************************************************
!
! Procedure: Get-Department-Details
! Description: Get Deptartment Details
!**************************************************************************
begin-procedure Get-Department-Details
 Let $Dept_Name = ''
Begin-SELECT
DEPT.DESCR
    Let $Dept_Name = &DEPT.DESCR
   FROM PS_DEPT_TBL DEPT
    WHERE DEPT.SETID  = $SetIDDept
      AND DEPT.DEPTID = $Deptid
      AND DEPT.EFFDT = (SELECT MAX(DEPT1.EFFDT) FROM PS_DEPT_TBL DEPT1
                            WHERE DEPT1.SETID  = DEPT.SETID
                              AND DEPT1.DEPTID = DEPT.DEPTID
                              AND DEPT1.EFFDT <= $SegEndDt
                       )
End-SELECT
end-procedure
!**************************************************************************
!
! Procedure: Get-Trans-Department-Details
! Description: Get Deptartment Details
!**************************************************************************
begin-procedure Get-Trans-Department-Details
 Let $Dept_Name = ''
Begin-SELECT
DEPTLNG.DESCR
    Let $Dept_Name = &DEPTLNG.DESCR
   FROM PS_DEPT_TBL_LANG DEPTLNG
    WHERE DEPTLNG.SETID  = $SetIDDept
      AND DEPTLNG.DEPTID = $Deptid
      AND DEPTLNG.EFFDT = (SELECT MAX(DEPTLNG1.EFFDT) FROM PS_DEPT_TBL_LANG DEPTLNG1
                            WHERE DEPTLNG1.SETID  = DEPTLNG.SETID
                              AND DEPTLNG1.DEPTID = DEPTLNG.DEPTID
                              AND DEPTLNG1.EFFDT <= $SegEndDt
                       )
      AND DEPTLNG.LANGUAGE_CD = $curr_language_cd
End-SELECT
end-procedure
!**************************************************************************
!
! Procedure: Get_Payslip_Labels
!
! Description: Get the payslip labels on delivery option for the payees
!              based on the paygroup
!
!**************************************************************************
begin-procedure Get_Payslip_Labels
begin-SELECT on-error=SQL-Error
PSLP.GPCN_DEL_OPT
PSLP.GPCN_LBL_ACUM1
PSLP.GPCN_LBL_ACUM2
PSLP.GPCN_LBL_ACUM3
PSLP.GPCN_LBL_ACUM4
PSLP.GPCN_LBL_ACUM5
PSLP.GPCN_LBL_ACUM6


!Check if an override for the delivery address exits
        let $Del_Opt               = &PSLP.GPCN_DEL_OPT

        do Check_Override_Add
!If the deleivery option is secondary get the details

        If $Del_opt = '60' !then get the secondary address
           let $Name = $EmployeeName
           Let $AdrType = 'MAIL'
           do Get_Address
           if $Adr2Found = 'N'
              let $Name = $EmployeeName
              do adformat
              Let $ADDLINE6 = ''
              do Compress-Address
           else
              Let $AdrType = 'MAIL'
              do Get_Address
              do adformat
              Let $ADDLINE6 = ''
              do Compress-Address
           End-if
        End-If

        If $curr_language_cd <> 'ENG'
               do Get_Payslip_Trans_Labels
               If rtrim($Acum_Lbl1, ' ') = ''
               Let $Acum_Lbl1 = rtrim(&PSLP.GPCN_LBL_ACUM1,' ')
               End-if
               If rtrim($Acum_Lbl2, ' ') = ''
               Let $Acum_Lbl2 = rtrim(&PSLP.GPCN_LBL_ACUM2,' ')
               End-if
               If rtrim($Acum_Lbl3, ' ') = ''
               Let $Acum_Lbl3 = rtrim(&PSLP.GPCN_LBL_ACUM3,' ')
               End-if
               If rtrim($Acum_Lbl4, ' ') = ''
               Let $Acum_Lbl4 = rtrim(&PSLP.GPCN_LBL_ACUM4,' ')
               End-if
               If rtrim($Acum_Lbl5, ' ') = ''
               Let $Acum_Lbl5 = rtrim(&PSLP.GPCN_LBL_ACUM5,' ')
               End-if
               If rtrim($Acum_Lbl6, ' ') = ''
               Let $Acum_Lbl6 = rtrim(&PSLP.GPCN_LBL_ACUM6,' ')
               End-if
        Else
               let $Acum_Lbl1      = &PSLP.GPCN_LBL_ACUM1
               let $Acum_Lbl2      = &PSLP.GPCN_LBL_ACUM2
               let $Acum_Lbl3      = &PSLP.GPCN_LBL_ACUM3
               let $Acum_Lbl4      = &PSLP.GPCN_LBL_ACUM4
               let $Acum_Lbl5      = &PSLP.GPCN_LBL_ACUM5
               let $Acum_Lbl6      = &PSLP.GPCN_LBL_ACUM6
        End-if

FROM PS_GPCN_PSLP PSLP,
     PS_GPCN_PYGRP_PSLP SGPAU
WHERE PSLP.EFFDT = (SELECT MAX(PSLP1.EFFDT)
                 FROM PS_GPCN_PSLP PSLP1
                 WHERE PSLP1.EFFDT <= $SegEndDt)
AND PSLP.GPCN_PAYSLIP_ID = SGPAU.GPCN_PAYSLIP_ID
AND GP_PAYGROUP = $GP_Paygroup
end-SELECT

end-procedure
!**************************************************************************
!
! Procedure: Get_Payslip_Trans_Labels
!
! Description: Get the payslip labels on delivery option for the payees
!              based on the paygroup
!
!**************************************************************************
begin-procedure Get_Payslip_Trans_Labels
begin-SELECT on-error=SQL-Error
PSLPLNG.GPCN_LBL_ACUM1
PSLPLNG.GPCN_LBL_ACUM2
PSLPLNG.GPCN_LBL_ACUM3
PSLPLNG.GPCN_LBL_ACUM4
PSLPLNG.GPCN_LBL_ACUM5
PSLPLNG.GPCN_LBL_ACUM6

               let $Acum_Lbl1      = &PSLPLNG.GPCN_LBL_ACUM1
               let $Acum_Lbl2      = &PSLPLNG.GPCN_LBL_ACUM2
               let $Acum_Lbl3      = &PSLPLNG.GPCN_LBL_ACUM3
               let $Acum_Lbl4      = &PSLPLNG.GPCN_LBL_ACUM4
               let $Acum_Lbl5      = &PSLPLNG.GPCN_LBL_ACUM5
               let $Acum_Lbl6      = &PSLPLNG.GPCN_LBL_ACUM6

FROM PS_GPCN_PSLP PSLPBASE,
     PS_GPCN_PSLP_LANG PSLPLNG
WHERE PSLPBASE.GPCN_PAYSLIP_ID = PSLPLNG.GPCN_PAYSLIP_ID
AND PSLPBASE.EFFDT = (SELECT MAX(PSLPBASE1.EFFDT)
                 FROM PS_GPCN_PSLP PSLPBASE1
                 WHERE PSLPBASE1.EFFDT <= $SegEndDt)
AND PSLPLNG.EFFDT = (SELECT MAX(PSLPLNG1.EFFDT)
                 FROM PS_GPCN_PSLP_LANG PSLPLNG1
                 WHERE PSLPLNG1.EFFDT <= $SegEndDt)
AND PSLPLNG.LANGUAGE_CD = $curr_language_cd
end-SELECT

end-procedure
!**************************************************************************
!
