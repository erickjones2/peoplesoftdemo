!**************************************************************************
! Procedure: Select-Parameters
!            Select-Parameters
!**************************************************************************
Begin-procedure Select-Parameters
begin-select

RC.CAL_RUN_ID
RC.GPCN_SORT_KEY1
RC.GPCN_SORT_KEY2
RC.GPCN_SORT_KEY3

FROM  PS_GPCN_RNCTL_PSLP RC
WHERE RC.OPRID          = $Prcs_OprID
