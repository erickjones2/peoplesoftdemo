!**************************************************************************
!
! Procedure: Define-Values
!
! Description: Define general SQL Dynamic Query Variables.
!
!**************************************************************************
begin-procedure Define-Values


Let $EmplList = 'N'
Let $DeptList = 'N'
Let $LocnList = 'N'
Let $PyenList = 'N'
Let $PyGPList = 'N'
Let $No_List  = 'Y'

!Sort order
! For CHN, we sort by PERS.NAME_AC.  This is where the Hun Yu Pin Yin (Latin
! version of the Chinese name) is stored.
  If $sort_key1 <> '00'
     evaluate $sort_key1
          when = '10'
                let $Sort_Order1  = 'ORDER BY PER.PAY_ENTITY '
          when = '20'
                let $Sort_Order1  = 'ORDER BY JOB.LOCATION '
          when = '30'
                let $Sort_Order1  = 'ORDER BY JOB.DEPTID '
          when = '40'
                let $Sort_Order1  = 'ORDER BY PERS.NAME_AC '
          When = '50'
                let $Sort_Order1  = 'ORDER BY PER.GP_PAYGROUP '
          When = '60'
                let $Sort_Order1  = 'ORDER BY PER.EMPLID '
          when-other
                break
     end-evaluate
  End-if
  If $sort_key2 <> '00'
     evaluate $sort_key2
          when = '10'
                let $Sort_Order2  = ', PER.PAY_ENTITY '
          when = '20'
                let $Sort_Order2  = ', JOB.LOCATION '
          when = '30'
                let $Sort_Order2  = ', JOB.DEPTID '
          when = '40'
                let $Sort_Order2  = ', PERS.NAME_AC '
          When = '50'
                let $Sort_Order2  = ', PER.GP_PAYGROUP '
          When = '60'
                let $Sort_Order2  = ', PER.EMPLID '
          when-other
                break
     end-evaluate
  End-if
  If $sort_key3 <> '00'
     evaluate $sort_key3
          when = '10'
                let $Sort_Order3  = ', PER.PAY_ENTITY '
          when = '20'
                let $Sort_Order3  = ', JOB.LOCATION '
          when = '30'
                let $Sort_Order3  = ', JOB.DEPTID '
          when = '40'
                let $Sort_Order3  = ', PERS.NAME_AC '
          When = '50'
                let $Sort_Order3  = ', PER.GP_PAYGROUP '
          When = '60'
                let $Sort_Order3  = ', PER.EMPLID '
          when-other
                break
     end-evaluate
  End-if

  String $Sort_order1 $Sort_order2 $Sort_order3 by ' ' into $sort_order

  if $Sort_Key1 = '00'
     let $Sort_order = 'ORDER BY PERS.NAME_AC '
  End-If
  if ($Sort_Key1 = '20' or $Sort_Key1 = '30') or ($Sort_Key2 = '20' or $Sort_Key2 = '30') or ($Sort_Key3 = '20' or $Sort_Key3 = '30')
#define colI 72
#define colI2 87
#define colI3 89O
  End-If

end-procedure
!**************************************************************************
! Procedure: Process-Main
! Description: Main processing section
!**************************************************************************
begin-procedure Process-Main

  Do Get-GP-Paygroup
  Do Process-Empl-List
  Do Process-Dept-List
  Do Process-Locn-List
  If $No_List = 'Y'
    Let $WhereClause_pyent = ''
    Let $WhereClause_pgrp = ''
    Do Process-Pay-Entity-List
    Do Process-PayGroup-List
    if $WhereClause_pyent <> ''  or $WhereClause_pgrp <> ''
        let $WhereClause = $WhereClause_pyent || ' ' || $WhereClause_pgrp
        let $No_List = 'N'
        Do Process-Payee
    end-if
  end-if

  Let #J = 1

  If $No_List = 'Y'
     While #J <= #I
        Get $GP_Pygrp FROM Gp_Paygroups(#J)
        If #j = 1
           Let $WhereTemp = ' AND (PER.GP_PAYGROUP = ' || '''' || $GP_Pygrp || ''''
        Else
           if rtrim($GP_Pygrp,' ') <> ''
              Let $TempString = ' OR PER.GP_PAYGROUP = ' || '''' || $GP_Pygrp || ''''
              Let $WhereTemp = $WhereTemp || $TempString
           End-if
        End-If
        Let #J = #J + 1
     End-While
     if rtrim($WhereTemp,' ') <> ''
        Let $WhereTemp = $WhereTemp || ')'
        let $WhereClause = $WhereTemp
        Do Process-Payee
     End-if
  End-If
  If( $custEnabled = 'Y')  
    DO update_stats
  End-If
end-procedure
