!
!**************************************************************************
! Procedure: Process-Pay Entity List
!            Process Pay Entity selected on Run Control Record
!**************************************************************************
Begin-Procedure Process-Pay-Entity-List
Let $No_List_pyent = 'Y'
let $WhereClause_pyent = ' AND PER.PAY_ENTITY IN ('
Begin-SELECT

DPY1.PAY_ENTITY &PayEntity
 display 'In Process-Pay-Entity-List SQL'

  If rtrim(&PayEntity, ' ') <> ''
     if $No_List_pyent = 'N'
        let $WhereClause_pyent = $WhereClause_pyent || ', '
     end-if
     let $PayEntity_t = rtrim(ltrim(&PayEntity, ' '), ' ')
     Let $WhereClause_pyent = $WhereClause_pyent || '''' || $PayEntity_t || ''''
     Let $No_List_pyent = 'N'
   End-If
FROM PS_GPCN_PSLP_PE_RC DPY1
WHERE DPY1.OPRID          = $Prcs_OprID
AND   DPY1.RUN_CNTL_ID    = $Prcs_Run_Cntl_ID
End-SELECT

if $No_List_pyent = 'Y'
    let $WhereClause_pyent = ''
else
    let $WhereClause_pyent = $WhereClause_pyent || ')'
end-if
End-Procedure
!
!**************************************************************************
! Procedure: Process-Paygroup List
!            Process Paygroup selected on Run Control Record
!**************************************************************************
Begin-Procedure Process-PayGroup-List
Let $No_List_pgrp = 'Y'
let $WhereClause_pgrp = ' AND PER.GP_PAYGROUP IN ('
Begin-SELECT
EPY1.GP_PAYGROUP &PayGrp

   If rtrim(&PayGrp, ' ') <> ''
     if $No_List_pgrp = 'N'
        let $WhereClause_pgrp = $WhereClause_pgrp || ', '
     end-if
     let $PayGrp_t = rtrim(ltrim(&PayGrp, ' '), ' ')
     Let $WhereClause_pgrp = $WhereClause_pgrp || '''' || $PayGrp_t || ''''
     Let $No_List_pgrp = 'N'
     Show 'Processing Paygroup : ' &PayGrp
!     Do Process-Payee
    End-If
    FROM PS_GPCN_PSLP_PG_RC EPY1
    WHERE EPY1.OPRID          = $Prcs_OprID
    AND   EPY1.RUN_CNTL_ID    = $Prcs_Run_Cntl_ID
End-SELECT
if $No_List_pgrp = 'Y'
    let $WhereClause_pgrp = ''
else
    let $WhereClause_pgrp = $WhereClause_pgrp || ')'
end-if
End-Procedure
