DEPT_BUD_DT.HP_TAX_ACCT
DEPT_BUD_DT.HP_CTX_ACCT

  let $DtDefault        = &DEPT_BUD_DT.HP_DFLT_FND_DT_FLG
  let $Ded_FundingEndDt = &DEPT_BUD_DT.FUND_END_DT_DED
  let $Tax_FundingEndDt = &DEPT_BUD_DT.FUND_END_DT_TAX

  if $DtDefault = 'Y'

     if &DEPT_BUD_DT.DEFAULT_FUND_OPTN = 'S'

        if $Ded_FundingEndDt = ''
           let $F_AcctCd = &DEPT_BUD_DT.ACCT_CD_DED
           do Get-Funding-End-Dt
           if $DtFound = 'Y'
              let $Ded_FundingEndDt = $EndDate
           end-if
        end-if

        if $Tax_FundingEndDt = ''
           let $F_AcctCd = &DEPT_BUD_DT.ACCT_CD_TAX
           do Get-Funding-End-Dt
           if $DtFound = 'Y'
              let $Tax_FundingEndDt = $EndDate
           end-if
        end-if

     end-if

  end-if

  if isnull($Ded_FundingEndDt)
     let $DedDate = 'NULL'
  else
     let $DedDate = '''' || $Ded_FundingEndDt || ''''
  end-if

  if isnull($Tax_FundingEndDt)
     let $TaxDate = 'NULL'
  else
     let $TaxDate = '''' || $Tax_FundingEndDt || ''''
  end-if

  do Insert-Into-Dept-Budget-Dt

FROM PS_DEPT_BUDGET_DT DEPT_BUD_DT
WHERE DEPT_BUD_DT.SETID = $Gbl_SetID
